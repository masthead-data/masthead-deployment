# Example terraform.tfvars file
# Copy this file to terraform.tfvars and customize the values

# Required: Your GCP project ID
project_id = "YOUR_PROJECT_ID"

# Optional: Enable/disable specific modules (all enabled by default)
# enable_modules = {
#   bigquery      = true
#   dataform      = true
#   dataplex      = true
#   analytics_hub = true
# }

# Optional: Enable required Google Cloud APIs (disabled by default)
# enable_apis = false

# Optional: Disable Private Log Viewer role (enabled by default)
# enable_privatelogviewer_role = true

# Optional: Add custom labels to all resources
# labels = {
#   environment = "production"
#   team        = "data"
#   cost_center = "engineering"
# }
